{
  "manifest_version": 3,
  "name": "Distractify",
  "version": "1.0",
  "icons": {
    "16": "brainlight.png",
    "48": "brainlight.png",
    "128": "brainlight.png"
  },
  "page_action": {
    "16": "brainlight.png",
    "48": "brainlight.png",
    "128": "brainlight.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "brainlight.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"],
      "html": ["index.html"]
    }
  ]
}
